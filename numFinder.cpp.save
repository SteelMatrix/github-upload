#include <iostream>
#include <fstream>
#include <vector>
#include <cmath>
using namespace std;
int main()
{
	vector<vector<int>> numberlist;
	numberlist.resize(10);	ifstream infile("data.txt");
	int val;
	while (infile >> val)
	{
		unsigned int index = floor(val/455000);
		numberlist[index].resize(numberlist[index].size() + 1);
		numberlist[index].push_back(val);
	}
	cout << "Success!" << endl;
	for (int i = 0; i < numberlist.size(); i++)
		for (int j = 0; j < numberlist[i].size(); j++)
			cout << numberlist[i][j] << " " ;
		cout << "\n";
	return 0;
}
